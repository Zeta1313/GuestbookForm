<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Contact Form - Admin</title>
  <link rel="icon" href="FormIcon.png">
  <link rel="stylesheet" href="FormCSS.css">
</head>
<body>
<header>
  <nav class="site-nav">
    <a href="/">Home</a>
    <a href="/form">Contact Form</a>
    <a href="/admin">Admin Page</a>
    <a href="https://linkedin.com/in/adrien-bertrand-b8614738a">LinkedIn</a>
    <a href="https://github.com/Zeta1313">Github</a>
  </nav>
</header>
<div class = "main-content">
        <table id = "order-table" class ="display">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Title</th>
                    <th>Company</th>
                    <th>LinkedIn</th>
                    <th>Email</th>
                    <th>Meeting</th>
                    <th>Details</th>
                    <th>Message</th>
                    <th>Format</th>
                    <th>Timestamp</th>
                </tr>
            </thead>
            <tbody>
              <% if (inputs.length === 0) { %>
                <tr>
                  <td colspan="10" style="text-align:center;">No submissions found</td>
                </tr>
              <% } else { %>
                <% inputs.forEach(input => { %>
                  <tr>
                    <td><%= input.fname %> <%= input.lname %></td>
                    <td><%= input.title %></td>
                    <td><%= input.company %></td>
                    <td><%= input.LN %></td>
                    <td><%= input.EA %></td>
                    <td><%= input.meeting %></td>
                    <td><%= input.addition %></td>
                    <td><%= input.message %></td>
                    <td><%= input.format %></td>
                    <td><%= input.timestamp %></td>
                  </tr>
                <% }) %>
              <% } %>
            </tbody>
        </table>
</div>
</body>
</html>